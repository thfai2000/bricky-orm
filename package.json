{
  "name": "llorm",
  "version": "0.0.5",
  "description": "Low-level Object Relation Mapper",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "npm install && rm -rf dist && rm -rf dist-test && concurrently -r \"tsc --project tsconfig.json --watch\" -r \"tsc --project tsconfig.test.json --watch\" ",
    "build": "npm install && rm -rf dist && tsc --project tsconfig.json",
    "test": "npm run test:sqlite",
    "test:mysql": "jest --watch --setupFiles=./dist-test/setup-mysql.js",
    "test:sqlite": "jest --watch --setupFiles=./dist-test/setup-sqlite.js",
    "mysql": "docker-compose down && docker-compose up",
    "test-mysql-db": "docker-compose down && concurrently -i -r \"jest --watch\" \"docker-compose up\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thfai2000/llorm.git"
  },
  "author": "Eddie Tang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thfai2000/llorm/issues"
  },
  "homepage": "https://github.com/thfai2000/llorm#readme",
  "runkitExampleFilename": "runkitexample.js",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.0.3",
    "@types/node-sql-parser": "^1.1.0",
    "@types/uuid": "^8.3.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "jest": "^27.0.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typescript": "^4.2.4",
    "uuid": "^3.4.0"
  },
  "dependencies": {
    "knex": "^0.95.5",
    "node-sql-parser": "^3.5.0"
  }
}
