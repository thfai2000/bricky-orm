{
  "name": "taichi-orm",
  "version": "0.1.5",
  "description": "Object Relation Mapper for SQL Database. Flexible and Strong",
  "keywords": [
    "data modeling",
    "orm",
    "sql",
    "database",
    "data query",
    "graphqL",
    "taichi",
    "knex",
    "mysql",
    "sqlite",
    "postegres",
    "high performance",
    "developer friendly"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "npm install && tsup src/index.ts --dts --watch src",
    "dev-test": "npm install && tsup test --dts --watch test --outDir dist-test",
    "test": "npm run test:sqlite",
    "test:mysql": "jest --watch --setupFiles=./dist-test/setup-mysql.js",
    "test:sqlite": "jest --watch --setupFiles=./dist-test/setup-sqlite.js",
    "test:pg": "jest --watch --setupFiles=./dist-test/setup-pg.js",
    "mysql": "docker-compose down && docker-compose up",
    "test-mysql-db": "docker-compose down && concurrently -i -r \"jest --watch\" \"docker-compose up\" ",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thfai2000/taichi-orm.git"
  },
  "author": "Eddie Tang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thfai2000/taichi-orm/issues"
  },
  "homepage": "https://github.com/thfai2000/taichi-orm#readme",
  "runkitExampleFilename": "runkit-example.js",
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.0.3",
    "@types/uuid": "^8.3.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "jest": "^27.0.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "pg": "^8.6.0",
    "sqlite3": "^4.2.0",
    "tsup": "^5.10.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2",
    "vuepress": "^2.0.0-beta.27"
  },
  "dependencies": {
    "knex": "^0.95.5"
  }
}
