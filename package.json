{
  "name": "taichi-orm",
  "version": "0.0.9",
  "description": "Object Relation Mapper for SQL Database. Flexible and Strong",
  "keywords": [
    "data modeling",
    "orm",
    "sql",
    "database",
    "data query",
    "graphqL",
    "taichi",
    "knex",
    "mysql",
    "sqlite",
    "postegres",
    "high performance",
    "developer friendly"
  ],
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "dev": "npm install && rm -rf dist && rm -rf dist-test && concurrently -r \"tsc --project tsconfig.json --watch\" -r \"tsc --project tsconfig.test.json --watch\" ",
    "build": "rm -rf dist && tsc --project tsconfig.json",
    "test": "npm run test:sqlite",
    "test:mysql": "jest --watch --setupFiles=./dist-test/setup-mysql.js",
    "test:sqlite": "jest --watch --setupFiles=./dist-test/setup-sqlite.js",
    "test:pg": "jest --watch --setupFiles=./dist-test/setup-pg.js",
    "mysql": "docker-compose down && docker-compose up",
    "test-mysql-db": "docker-compose down && concurrently -i -r \"jest --watch\" \"docker-compose up\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thfai2000/taichi-orm.git"
  },
  "author": "Eddie Tang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thfai2000/taichi-orm/issues"
  },
  "homepage": "https://github.com/thfai2000/taichi-orm#readme",
  "runkitExampleFilename": "runkit-example.js",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.0.3",
    "@types/uuid": "^8.3.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "jest": "^27.0.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "knex": "^0.95.5",
    "uuid": "^3.4.0"
  }
}
